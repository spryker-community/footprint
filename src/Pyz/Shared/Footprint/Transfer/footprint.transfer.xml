<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="FootprintProcessorResult">
        <property name="isSuccessful" type="bool"/>
        <property name="errorMessage" type="string"/>
    </transfer>

    <transfer name="FootprintTemplate">
        <property name="templateName" type="string"/>
        <property name="moduleName" type="string"/>
        <property name="options" type="FootprintOption[]" singular="option"/>
    </transfer>

    <transfer name="FootprintOption">
        <property name="name" type="string"/>
        <property name="label" type="string"/>
        <property name="isApplicable" type="bool"/>
    </transfer>

    <transfer name="FootprintTemplatePath">
        <property name="origin" type="string"/>
        <property name="target" type="string"/>
    </transfer>

</transfers>
